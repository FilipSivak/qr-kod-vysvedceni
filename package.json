{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "Aplikace pro čtení QR kódu z vysvědčení",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "clean": "rm -rf windows_installer QRKodVysvedceni",
    "pyinstaller": "cd python && conda run -n electron pyinstaller -y --onefile script.py && cp dist/script.exe ..",
    "package": "electron-packager . --ignore=python --overwrite --platform=win32 --arch=x64 QRKodVysvedceni",
    "installer": "node build_installer.js"
  },
  "repository": "https://github.com/FilipSivak/qr-kod-vysvedceni",
  "keywords": [
    "QR",
    "code",
    "Czech republic",
    "report card"
  ],
  "author": "GitHub",
  "license": "GPLv3",
  "devDependencies": {
    "electron": "^8.2.0",
    "electron-packager": "^14.2.1",
    "electron-rebuild": "^1.10.1",
    "electron-wix-msi": "^2.2.0"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "keytar": "^5.4.0",
    "windows-credman": "^1.0.1"
  }
}
